  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "outDir": "build",
    "esModuleInterop" : true,
    "lib": ["esnext", "dom"],
    "skipLibCheck": true,
    "jsx": "react",
    "moduleResolution": "node",
    "forceConsistentCasingInFileNames": true
  },
  "include": ["src"],
  "exclude": ["node_modules"]
}